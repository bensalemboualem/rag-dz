# ============================================
# IAFACTORY VIDEO STUDIO PRO - CONFIGURATION
# ============================================
# Copiez ce fichier en .env et remplissez les valeurs

# === APPLICATION ===
DEBUG=false
ENVIRONMENT=development
SECRET_KEY=your-super-secret-key-change-this-in-production

# === DATABASE ===
DB_PASSWORD=your-postgres-password

# === MINIO (Stockage) ===
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=your-minio-secret-key

# === APIS IA ===

# Anthropic (Claude) - REQUIS
ANTHROPIC_API_KEY=sk-ant-api03-xxx

# ElevenLabs (TTS) - REQUIS pour la voix
ELEVENLABS_API_KEY=xxx

# MiniMax/Hailuo (Video Generation) - REQUIS pour la vidéo
MINIMAX_API_KEY=xxx
MINIMAX_GROUP_ID=xxx

# Fal.ai (Images & Video) - Optionnel
FAL_API_KEY=xxx

# Suno AI (Musique) - Optionnel
SUNO_API_KEY=xxx

# Grok/xAI (Growth Hacker) - Optionnel
GROK_API_KEY=xxx

# === N8N (Automation) ===
N8N_USER=admin
N8N_PASSWORD=your-n8n-password
N8N_API_KEY=xxx

# === MONITORING ===
GRAFANA_USER=admin
GRAFANA_PASSWORD=your-grafana-password

# === SENTRY (Error Tracking) ===
SENTRY_DSN=https://xxx@sentry.io/xxx

# === OAUTH (Optionnel) ===
GOOGLE_CLIENT_ID=xxx
GOOGLE_CLIENT_SECRET=xxx

# === YOUTUBE API ===
YOUTUBE_CLIENT_ID=xxx
YOUTUBE_CLIENT_SECRET=xxx

# === TIKTOK API ===
TIKTOK_CLIENT_KEY=xxx
TIKTOK_CLIENT_SECRET=xxx

# === INSTAGRAM/META API ===
META_APP_ID=xxx
META_APP_SECRET=xxx

# === AUTRES ===
# Timezone
TZ=Africa/Algiers

# Langue par défaut
DEFAULT_LANGUAGE=fr
