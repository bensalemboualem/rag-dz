{
  "name": "@iafactory-dz/seo-dz-boost",
  "version": "1.0.0",
  "description": "Module SEO pour iaFactory Algeria - Landing pages et optimisation Google Algérie",
  "private": true,
  "type": "module",
  "main": "src/components/index.ts",
  "types": "src/components/index.ts",
  "keywords": [
    "seo",
    "algeria",
    "iafactory",
    "rag",
    "ia-algerie",
    "google-ranking",
    "landing-pages"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "docker:build": "docker build -t iaf-dz-seo .",
    "docker:run": "docker run -p 8218:80 iaf-dz-seo",
    "sitemap:validate": "xmllint --schema http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd public/sitemap.xml --noout"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "seo": {
    "targetDomain": "www.iafactoryalgeria.com",
    "targetMarket": "Algeria",
    "primaryKeywords": [
      "IA Algérie",
      "RAG Algérie",
      "assistant fiscal Algérie",
      "assistant juridique Algérie",
      "création entreprise Algérie IA",
      "API IA Algérie",
      "Pack PME DZ"
    ],
    "languages": ["fr-DZ", "ar-DZ"],
    "analytics": {
      "supported": ["ga4", "plausible", "matomo"]
    }
  }
}
